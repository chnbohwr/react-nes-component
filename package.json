{
  "name": "react-nes-component",
  "description": "react component for 8 bit style",
  "version": "1.0.1",
  "main": "bin/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/chnbohwr/react-nes-component"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "start": "webpack-dev-server --mode development --hot --config webpack.config.js",
